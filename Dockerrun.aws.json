{
    "AWSEBDockerrunVersion": 2,
    "containerdefinitions": [
        {
            "name": "client",
            "image": "currentanalyticsio/multi-client",
            "hostname": "client", //Comes from the services in docker-compose.yml
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "currentanalyticsio/multi-server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "currentanalyticsio/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "currentanalyticsio/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }
    ]
}

